{
  "rewrites": [
    {
      "source": "/user/:path*",
      "destination": "https://tomas-back.onrender.com/user/:path*"
    },
    {
      "source": "/books/:path*",
      "destination": "https://tomas-back.onrender.com/books/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
